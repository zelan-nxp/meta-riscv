<?xml version="1.0" encoding="UTF-8"?>

<manifest>

  <remote name="github" fetch="https://github.com/" alias="origin"/>
  <remote name="yocto"  fetch="https://git.yoctoproject.org/" alias="origin" />
  <default remote="github" revision="master" />

  <project name="zelan-nxp/meta-riscv.git"            path="layers/meta-riscv">
    <linkfile src="tools/setup.sh"  dest="setup.sh"/>
  </project>
  <project name="openembedded/openembedded-core.git"  path="layers/openembedded-core" />
  <project name="openembedded/bitbake.git"            path="layers/bitbake"           />
  <project name="openembedded/meta-openembedded.git"  path="layers/meta-openembedded" />
  <project name="meta-yocto"   remote="yocto"         path="layers/meta-yocto" />

</manifest>
